OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/BRCocoaLumberjack" -isystem "${PODS_ROOT}/Headers/Public/BRStyle" -isystem "${PODS_ROOT}/Headers/Public/CocoaLumberjack" -isystem "${PODS_ROOT}/Headers/Public/Color-Picker-for-iOS" -isystem "${PODS_ROOT}/Headers/Public/OCHamcrest"
